<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="500.0" maxWidth="450.0" minHeight="500.0" minWidth="450.0" prefHeight="500.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.playervsplayer.FXMLPlayerVsPlayerController">
   <top>
      <AnchorPane prefHeight="0.0" prefWidth="450.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="backBtn" layoutX="14.0" layoutY="8.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="30.0" prefWidth="30.0" style="-fx-border-radius: 30px; -fx-background-radius: 30px;" text="B" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="14.0" />
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextField layoutX="85.0" layoutY="135.0" prefHeight="40.0" prefWidth="280.0" promptText="Player 1 name" />
            <Label layoutX="183.0" layoutY="100.0" text="Player 1">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField layoutX="85.0" layoutY="242.0" prefHeight="40.0" prefWidth="280.0" promptText="Player 2 name" />
            <Label layoutX="183.0" layoutY="207.0" text="Player 2">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="startBtn" layoutX="165.0" layoutY="366.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Start">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Text fx:id="playerVsPlayerLabel" layoutX="100.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player Vs Player" textAlignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
