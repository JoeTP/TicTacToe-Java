<BorderPane maxHeight="720.0" maxWidth="720.0" minHeight="500.0" minWidth="450.0" prefHeight="500.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.playervscomp.FXMLPlayerVsCompController">
   <center>
      <AnchorPane prefHeight="472.0" prefWidth="720.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="chooseDifficultyLabel" layoutX="98.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose Difficulty" textAlignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Slider fx:id="difficultySlider" blockIncrement="1.0" layoutX="82.0" layoutY="273.0" majorTickUnit="1.0" max="2.0" minorTickCount="0" prefHeight="31.0" prefWidth="286.0" snapToTicks="true" />
            <ImageView fx:id="difficultyImg" fitHeight="144.0" fitWidth="262.0" layoutX="96.0" layoutY="106.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/assets/icons/easy.PNG" />
               </image>
            </ImageView>
            <Button fx:id="startBtn" layoutX="165.0" layoutY="318.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Start" AnchorPane.leftAnchor="165.0" AnchorPane.rightAnchor="165.0">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
   <top>
      <AnchorPane prefHeight="59.0" prefWidth="720.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="backBtn" layoutX="20.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="30.0" prefWidth="30.0" style="-fx-border-radius: 30px; -fx-background-radius: 30px;" text="B" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="14.0" />
         </children>
      </AnchorPane>
   </top>
</BorderPane>
